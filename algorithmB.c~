#include <stdio.h>
#include <stdlib.h>

int main(){
  int count=0;
  int* height=(int*)malloc(sizeof(int));
  long long** pascal=(long long**)malloc(sizeof(long long));
  scanf("%d",&count);
  for (int i=0;i<count;i++){
    scanf("%d",&height[i]);
    printf("%d\n",height[i]);
  }
  for (int i=0;i<count;i++){
    pascal=NULL;
    for(int j=0;j<height[i];j++){
      pascal[j][0]=1;
      pascal[j][j]=1;
      for (int k=1;k<j;k++){
	pascal[j][k]=pascal[j][k-1]+[j-1][k];
	printf("%d ",pascal[j][k]);
      }
  }
  return 0;
}
